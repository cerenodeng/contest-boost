generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model NavTab {
  id      Int     @id @default(autoincrement())
  label   String
  content String
}

model Contest {
  id            Int     @id @default(autoincrement())
  title         String
  startDatetime String
  endDatetime   String
  content       String
}